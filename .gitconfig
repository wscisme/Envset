[user]
        name = ***
        email = ***@**.**
[core]
        editor = emacs -q
[color]
        branch = auto
        status = auto
        diff = auto
[alias]
        co = checkout
        ci = commit
        cm = commit -m
        ca = commit -a
        cam = commit -am
        st = status
        br = branch
        sh = stash
        hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
        type = cat-file -t
        dump = cat-file -p
        rsth = reset --hard
